{
	"name": "tabby",
	"version": "0.1",
	"manifest_version": 2,

	"description": "Chrome extension to encourage and foster good browsing habits",
	"icons": {
		"128": "./img/tabby_128.png"
	},
	"browser_action": {
		"default_icon": "./img/tabby_16.png",
		"default_popup": "popup.html",
		"default_title": "tabby"
	},
	"content_scripts": [ {
	"js": [ "js/lib/jquery-1.8.2.min.js", "js/app/content.js" ],
	"matches": [ "*://*/*" ],
	"run_at": "document_start"
	} ],
	"minimum_chrome_version": "18",
	"permissions": [ "http://*/*", "https://*/*", "unlimitedStorage", "contextMenus", "cookies", "tabs", "notifications" ],
	 "background": {
    "scripts": ["./js/app/popup.js"]
  }
}
