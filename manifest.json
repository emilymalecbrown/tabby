{
	"name": "tabby",
	"version": "0.1",
	"manifest_version": 2,
	"description": "A simple Chrome extension to reign in those with bad tabits.",
	"icons": {
		"128": "./img/tabby_128.png",
		"16": "./img/tabby_16.png"
	},
	"browser_action": {
		"default_popup": "popup.html",
		"default_title": "tabby"
	},
	"minimum_chrome_version": "18",
	"permissions": [
		"tabs", "notifications", "storage"
	],
	 "background": {
    "scripts": ["./js/app/popup.js"]
  }
}
